{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DenyEveryoneUnlessSith",
            "Effect": "Deny",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::exegol-wayfinder-vault-7/*",
            "Condition": {
                "StringNotEquals": {
                    "aws:UserAgent": "Sith-Holonet-Uplink/v6.6",
                    "aws:Referer": "https://internal.empire.gov/dark-council"
                }
            }
        },
        {
            "Sid": "AllowPublicReadCheck",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::exegol-wayfinder-vault-7/*"
        }
    ]
}
